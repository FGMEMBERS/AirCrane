    </offsets>
  </model>

  <model>
    <name>Blade4</name>
    <path>Aircraft/AirCrane/Models/MainRotor/blade4.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>      0.000 </roll-deg>
      <pitch-deg>     0.000 </pitch-deg>
      <heading-deg> 180.000 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade5</name>
    <path>Aircraft/AirCrane/Models/MainRotor/blade5.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>      0.000 </roll-deg>
      <pitch-deg>     0.000 </pitch-deg>
      <heading-deg> 240.000 </heading-deg>
    </offsets>
  </model>

  <model>
    <name>Blade6</name>
    <path>Aircraft/AirCrane/Models/MainRotor/blade6.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>      0.000 </roll-deg>
      <pitch-deg>     0.000 </pitch-deg>
      <heading-deg> 300.000 </heading-deg>
    </offsets>
  </model>

  <animation>
    <name>touteslespales</name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
    <object-name>Blade4</object-name>
    <object-name>Blade5</object-name>
    <object-name>Blade6</object-name>
  </animation>

