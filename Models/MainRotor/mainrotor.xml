  <animation>
    <name>rotorcomplet</name>
    <object-name>rotor</object-name>
    <object-name>assemble</object-name>
    <object-name>plateau</object-name>
    <object-name>pistons</object-name>
    <object-name>axeH</object-name>
    <object-name>axeV</object-name>
    <object-name>Blade1</object-name>
    <object-name>Blade2</object-name>
    <object-name>Blade3</object-name>
    <object-name>Blade4</object-name>
    <object-name>Blade5</object-name>
    <object-name>Blade6</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>rotorcomplet</object-name>
    <property>rotors/main/blade[0]/position-deg</property>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x>  0.000 </x>
      <y>  0.000 </y>
      <z>  1.000 </z>
    </axis>
  </animation>

