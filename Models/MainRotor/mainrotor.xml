<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>mainrotor.ac</path>

  <animation>
    <object-name>rotor</object-name>
    <object-name>assemble</object-name>
    <object-name>axeH</object-name>
    <object-name>axeV</object-name>
    <object-name>plateau</object-name>
    <object-name>pistons</object-name>
  </animation>

  <!-- Blades -->
  <model>
    <name>Blade1</name>
    <path>Aircraft/AirCrane/Models/MainRotor/blade1.xml</path>
    <offsets>
      <x-m> 0.0 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> 0.0 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

  <model>
