      <entry><ind> 0.000 </ind><dep> -10 </dep></entry>
      <entry><ind> 0.086 </ind><dep>   0 </dep></entry>
      <entry><ind> 1.000 </ind><dep>  14 </dep></entry>
    </interpolation>
    <center>
      <x-m> -8.702 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -3.302 </z-m>
    </center>
    <axis>
      <x> 0.0 </x>
      <y> 1.0 </y>
      <z> 0.0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>RoueGauche</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep> -0.10 </dep></entry>
      <entry><ind> 0.29 </ind><dep>  0.00 </dep></entry>
      <entry><ind> 1.00 </ind><dep>  0.28 </dep></entry>
    </interpolation>
    <axis>
      <x> 0.0 </x>
      <y> 0.0 </y>
      <z> 1.0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>rotate</type>
    <object-name>articuleGH</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep>  10 </dep></entry>
      <entry><ind> 0.29 </ind><dep>   0 </dep></entry>
      <entry><ind> 1.00 </ind><dep> -27 </dep></entry>
    </interpolation>
    <center>
      <x-m> -1.081 </x-m>
      <y-m> -3.027 </y-m>
      <z-m> -2.465 </z-m>
    </center>
    <axis>
      <x> 0.0 </x>
      <y> 1.0 </y>
      <z> 0.0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>rotate</type>
